	gcc -c -Wall -Wextra -Werror ft_toupper.c

libft.a: $(objects)
	ar -cr libft.a $(objects)

clean:
	rm -rf $(objects) $(bonusobjects)

fclean:
	rm -rf $(objects) $(bonusobjects) libft.a

re: 
	Make fclean
	Make all

bonus: $(objects) $(bonusobjects)
	ar -cr libft.a $(objects) $(bonusobjects)